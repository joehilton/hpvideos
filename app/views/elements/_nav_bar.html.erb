	<% if user_signed_in? %>
		<%= link_to "Sign Out", destroy_user_session_path, :method => :delete, :class => "sign_out" %>
		<%= link_to "Upload", "#button", :rel => "leanModal", :class => "upload" %>
		<%= link_to "Profile", users_path, :method => :get, :class => "profile" %>
		<%= link_to "Home", root_path, :method => :get, :class => "home" %>
	<% else %>
		<%= link_to "Sign Up", "#sign_up", :rel => "leanModal", :class => "sign_up" %>
		<%= link_to "Sign In", "#sign_in", :rel => "leanModal", :class => "sign_in" %>
	<% end %>
